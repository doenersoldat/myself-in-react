{
	"name": "myself-in-react",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"lint": "yarn lint:ts && yarn lint:style",
		"lint:ts": "eslint src/**/*.{js,jsx,ts,tsx}",
		"lint:style": "stylelint src/**/*.{css,scss,sass}",
		"format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,scss,md,json} --config ./.prettierrc.js"
	},
	"dependencies": {
		"i18next": "^21.9.0",
		"i18next-browser-languagedetector": "^6.1.5",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-i18next": "^11.18.3",
		"sass": "^1.54.4"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.3.0",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"@types/react-test-renderer": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"@vitejs/plugin-react": "^1.3.0",
		"c8": "^7.12.0",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.4.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"jsdom": "^20.0.0",
		"prettier": "^2.7.1",
		"react-test-renderer": "^18.2.0",
		"stylelint": "^14.10.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-standard": "^27.0.0",
		"stylelint-config-standard-scss": "^5.0.0",
		"typescript": "^4.6.3",
		"vite": "^2.9.15",
		"vitest": "^0.21.1"
	}
}
